---
source: src/builder/tests/misc/sort.rs
expression: "sqlformat::format(query.as_str(), &sqlformat::QueryParams::None,\n                  sqlformat::FormatOptions::default())"

---
WITH "cte_peoples_data" AS (
  SELECT
    "public"."peoples"."id" :: TEXT AS "id",
    $1 :: TEXT AS "type",
    JSON_BUILD_OBJECT(
      $2,
      "public"."peoples"."age",
      $3,
      "public"."peoples"."first-name",
      $4,
      "public"."peoples"."gender",
      $5,
      "public"."peoples"."last-name",
      $6,
      "public"."peoples"."twitter"
    ) AS "data",
    "public"."peoples"."id" :: UUID AS "_id",
    "public"."peoples"."favorite_color" AS "rel_favorite_color"
  FROM
    "public"."peoples"
),
"cte_rel_articles_rel_data" AS (
  SELECT
    "public"."people-article"."id" :: TEXT AS "id",
    $7 :: TEXT AS "type",
    NULL :: json AS "data",
    "public"."people-article"."id" :: UUID AS "_id",
    "public"."people-article"."article_id" AS "rel_article_id",
    "public"."people-article"."people_id" AS "rel_people_id"
  FROM
    "public"."people-article"
    INNER JOIN "cte_peoples_data" ON "public"."people-article"."people_id" :: TEXT = "cte_peoples_data"."id" :: TEXT
),
"cte_rel_articles_data" AS (
  SELECT
    "public"."articles"."id" :: TEXT AS "id",
    $8 :: TEXT AS "type",
    NULL :: json AS "data",
    "public"."articles"."id" :: UUID AS "_id",
    "public"."articles"."title" AS "sort_title"
  FROM
    "public"."articles"
    INNER JOIN "cte_rel_articles_rel_data" ON "cte_rel_articles_rel_data"."rel_article_id" :: TEXT = "public"."articles"."id" :: TEXT
)(
  SELECT
    "cte_rel_articles_data"."id",
    "cte_rel_articles_data"."type",
    "cte_rel_articles_data"."data"
  FROM
    "cte_rel_articles_data"
)
UNION
ALL (
  SELECT
    "cte_rel_articles_rel_data"."id",
    "cte_rel_articles_rel_data"."type",
    "cte_rel_articles_rel_data"."data"
  FROM
    "cte_rel_articles_rel_data"
)
UNION
ALL (
  SELECT
    "cte_peoples_data"."id",
    "cte_peoples_data"."type",
    "cte_peoples_data"."data"
  FROM
    "cte_peoples_data"
    INNER JOIN "public"."people-article" ON "public"."people-article"."people_id" = "cte_peoples_data"."_id"
    INNER JOIN "cte_rel_articles_data" ON "cte_rel_articles_data"."_id" = "public"."people-article"."article_id"
  ORDER BY
    "cte_rel_articles_data"."sort_title" ASC
);
