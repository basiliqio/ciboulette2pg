"cte_favorite_color_data" AS (
  SELECT
    "cte_peoples_main"."id",
    "public"."favorite_color"."color"
  FROM
    "cte_peoples_main"
    INNER JOIN "public"."peoples" ON "public"."peoples"."id" = "cte_peoples_main"."id"
    INNER JOIN "public"."favorite_color" ON "public"."favorite_color"."id" = "public"."peoples"."favorite_color";
