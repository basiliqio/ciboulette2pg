SELECT
  "public"."favorite_color"."id" :: TEXT AS "id",
  $0 :: TEXT AS "type",
  NULL :: json AS "data"
FROM
  "public"."favorite_color"
WHERE
  "public"."favorite_color"."id" IN (
    SELECT
      "public"."cte_peoples"."favorite_color"
    FROM
      "public"."cte_peoples"
